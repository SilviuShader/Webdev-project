@model Crowd_knowledge_contribution.Models.Category
@{
    ViewBag.Title = "Domeniu";
}

<h2>@ViewBag.Title</h2>
<p>@Model.CategoryName</p>

<form method="post" action="/Categories/Delete/@Model.CategoryId">
    @Html.HttpMethodOverride(HttpVerbs.Delete)
    <button class="btn btn-success" type="submit">Șterge domeniul</button>

</form>
<br />
<a class="btn btn-success" href="/Categories/Index/">Afișare listă domenii</a>

